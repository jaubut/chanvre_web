.blogshow
    height 25rem
    width auto
    position relative
    background-position center center
    background-size cover
    background-repeat no-repeat
    border-radius 2%
    box-shadow 0px 1px 2px 0px #D6D6D8

.blogindex
    background-color white
    height 100px
    width 100%
    border-bottom-left-radius 2%
    border-bottom-right-radius 2%
    position absolute
    bottom 0px
    z-index 80 !important
    
    p
        color black
        text-shadow none
        text-align center
        font-weight 400
        line-height normal
    .auteur
        font-size 1rem
        color #F9B42D

#indexblog
    width 100%
    
#instagram
    width 100%
        
#blog
    img
        max-width 100%
    .auteur
        font-size 1rem
        color #F9B42D
    .tag
        display inline
        margin-right 10px
        font-weight 400
        text-transform uppercase
    .round-text
        border 1px solid burlywood
        border-radius 100%
        width 45px
    time
        display inline
        font-size 0.75rem
    li
        font-size 0.95rem
        line-height 1.75rem
    figcaption
        font-size 0.85rem
    @media (min-width: 800px)
        width 70%
    @media (max-width: 800px)
        width 100%
#photo
       
    .photo
          height 300px
          display inline-block
          background-position center center
          background-size cover
          background-repeat no-repeat
          cursor pointer
          position relative
          overflow hidden
          border-radius 2%
          &:hover
            filter grayscale(10%)
        
    #instafeed
        width 100vw
        overflow hidden
#seqoia
    img
        height 500px
    .jumbosection
        background-size cover
        background-position center center
        background-repeat no-repeat
        height 400px
        width 100%
        z-index 100 !important
        display inline-block
.m-bottom
  margin-bottom 48px
#section
    display block
    position sticky
    top 25px
    margin-right -100px
.container
  padding-top 100px

.body-content
  background #fff
  padding 100px 0 120px 0
  position relative
  z-index 0 !important
  & > .container
    position relative
    top 100px
    padding-bottom 100px

.jumbotron
  background-size cover
  background-position center center
  background-repeat no-repeat
  height 760px
  width 100%
  z-index 100 !important

.jumbotron h1
    color #4D4D4D
.jumbotron h2
  color #4D4D4D
  font-size 1.5rem
  margin-top 20px

button
  height 48px
  width 180px
  text-align center
  color #fff
  font-size 1rem
  font-weight 200
  background transparent
  border 1px solid #fff
  border-radius 25px
  margin 20px 0
  cursor pointer
  transition all 0.3s ease
  outline none
  &:hover
    background #75C050
    color #fff
    transform: scale3d(1.05, 1.05, 1.05)
  &:active
    outline none

.btn-secondary
  border 1px solid #3E7BFE
  background #3E7BFE
  color white
  &:hover
    border 1px solid #23b39d
    background #23b39d
    color white
.don
    background #F14E75
    &:hover
        background #FBCED1
.btn-recette
  border 1px solid #75c050
  color #75c050
  &:hover
    background #75c050
    color #fff
//couleur
.green-color
    color #75c050
span.highlight
    background-color #FDF66C
    box-shadow 2px 0 0 #FDF66C, -2px 0 0 #FDF66C
    padding 2px 0


.highlight-container:before, .highlight-container:after
    content " "
    display block
    height 90%
    width 100%
    margin-left -3px
    margin-right -3px
    position absolute
.line
  height 1px
  width 100%
  background #D6D6D8
  margin 80px 0

.grid-display
  margin 20px 0
  [class^="col"]
    background #FBCED1
    border 1px solid #75c050
    border-radius 4px
    box-sizing border-box
    text-align center
    p
      color #75c050
      font-size 0.8rem

pre
  margin-top 30px
  overflow auto
  background #fff
  border-radius 4px
  border 1px solid #D8DCE6
  padding 24px 0

code,
.tag,
.el,
.name,
.content,
.comment
  font-family monospace
  font-size 1.1rem
  line-height 1.2rem
  color #636366

.el
  color #75c050

.name
  color #F9B42D

.content
  color #75C050

.comment
  color #B6CAD2
  
.lightbox
    display none
    position fixed
    z-index 999
    width 100%
    height 100%
    text-align center
    top 0
    left 0
    background rgba(255, 255, 255, 0.95)
    img 
        max-width 90%
        max-height 80%
        margin-top 2%
    &:target
        outline none
        display block

footer
  background #B1E95C
  background #75C050
  background-image linear-gradient(#75C050, #B1E95C)
  padding-bottom 80px
  p
    color #fff
    font-size 1rem
  img  
      height 3rem

  a
      text-decoration none
      color #fff
      transition all 0.3s ease
      &:hover
        text-decoration line-through
        color white
  .anne
      font-size 0.75rem
  strong
      color white
#blogfooter
    width 100%
    p 
        color #4D4D4D
    a 
        color #4D4D4D
    strong
        color #4D4D4D
    .i-footer
        color #4D4D4D

.img
  background-size contain
  background-repeat no-repeat

.img-logo
  background-image url('img/navlogo.svg')
  background-size contain
  height 80px
  width 80px
  margin-bottom 60px
  -webkit-animation: title-fall 0.75s ease;
  -moz-animation: title-fall 0.75s ease;
  animation: title-fall 0.75s ease;

.img-website-mock
  position relative
  background-image url('img/jechanvreweb.svg')
  height 0
  padding-top 71.8%
  margin-top 80px
  z-index 200 !important

.img-responsive
  background-image url('img/responsive.png')
  height 96px
  width 144px

.img-lightweight
  background-image url('img/light.png')
  height 96px
  width 96px

.img-simple
  background-image url('img/simple.png')
  height 96px
  width 132px

@media only screen and (min-width: 800px)
  .jumbotron
    height 600px
    
  #blogfooter
    width 70%
    
  .btn-offre
      margin-top -100px
      
@keyframes pulse 
  0% 
    transform: scale3d(1, 1, 1)
  
  100%
    transform: scale3d(1.1, 1.1, 1.1)
  
  100% 
    transform: scale3d(1, 1, 1)
  



