header
    #blogheader
        a(href="#{ creditref }")
            .side-image(style=(image))
                .credit-photo= creditphoto
        .container
                .row
                    .col-12
                          a.goback(href='/index')
                            i.fa.fa-home.i-header(aria-hidden='true')
                          a.goback(href='/blog')
                            i.fa.fa-book.i-header(aria-hidden='true')
                          a.goback.btn-print(onclick='myPrint()')
                            i.fa.fa-print.i-header(aria-hidden='true')
